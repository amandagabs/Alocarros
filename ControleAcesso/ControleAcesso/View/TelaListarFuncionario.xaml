<Window x:Class="ControleAcesso.View.TelaListarFuncionario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControleAcesso.View"
        mc:Ignorable="d"
        Title="TelaListarFuncionario" Height="450" Width="800">
    <Grid Background="#FFC8C8C8">
        <TextBox x:Name="Nome" HorizontalAlignment="Left" Height="23" Margin="44,83,0,0" TextWrapping="Wrap" Text="Nome" VerticalAlignment="Top" Width="208"/>
        <Button x:Name="Voltar" Content="Voltar" HorizontalAlignment="Left" Margin="45,120,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="Cadastrar" Content="Editar" HorizontalAlignment="Left" Margin="177,120,0,0" VerticalAlignment="Top" Width="74"/>
        <Label Content="Listar Funcionário" HorizontalAlignment="Left" Margin="29,10,0,0" VerticalAlignment="Top" Height="71" Width="415" FontFamily="Calibri" FontWeight="Bold" FontSize="48"/>
        <Button x:Name="Cadastrar_Copy" Content="Pesquisar" HorizontalAlignment="Left" Margin="260,85,0,0" VerticalAlignment="Top" Width="74"/>

    </Grid>
</Window>
